config interface 'loopback'
	option enable '1'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option enable '0'
	option ifname 'eth0'
	option proto 'static'
	option ipaddr '192.168.2.30'
	option netmask '255.255.255.0'
    option mac  '00:03:7F:FF:FF:30'

config interface 'hsuart'
    option enable '0'

config interface 'usbnet_gadget'
	option enable '0'
	option ifname 'usb0'
	option proto 'static'
	option ipaddr '192.168.40.254'
	option netmask '255.255.255.0'

config interface 'usbnet_ether'
	option enable '1'
	option ifname 'eth0'
	option proto 'static'
	option ipaddr '192.168.42.254'
	option netmask '255.255.255.0'

config interface 'camsta'
	option enable '0'
	option ifname 'ath1'
	option proto 'static'
	option ipaddr '192.168.40.100'
	option netmask '255.255.255.0'

config interface 'fwdusb'
	option enable '1'
	option ifname 'usb0'
	option proto 'dhcp'
	option ipaddr '192.168.42.48'
	option netmask '255.255.255.0'

config interface 'fwdusb_remcntl'
	option enable '1'
	option ifname 'usb0:1'
	option proto 'static'
	option ipaddr '192.168.42.100'
	option netmask '255.255.255.0'
	option port '1234'


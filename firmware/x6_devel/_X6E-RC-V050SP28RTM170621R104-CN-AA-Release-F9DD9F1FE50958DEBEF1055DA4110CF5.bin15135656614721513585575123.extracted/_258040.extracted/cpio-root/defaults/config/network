        option netmask '255.255.255.0'

config interface 'camap'
	option enable '1'
	option ifname 'ath0'
	option proto 'static'
	option ipaddr '192.168.40.100'
	option netmask '255.255.255.0'
	option dhcpd_start '192.168.40.200'

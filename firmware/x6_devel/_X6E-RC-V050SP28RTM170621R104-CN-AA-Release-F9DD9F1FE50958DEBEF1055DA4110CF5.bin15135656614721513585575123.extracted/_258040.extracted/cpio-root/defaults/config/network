config interface 'fwdap'
	option enable '1'
	option ifname 'wlan0'
	option proto 'static'
	option ipaddr '192.168.100.1'
	option netmask '255.255.255.0'
	option dhcpd_start '192.168.100.100'
	option dhcpd_end '192.168.100.200'

config interface 'fwdap_remcntl'
	option enable '1'
	option ifname 'wlan0'
	option proto 'static'
	option ipaddr '192.168.100.1'
	option netmask '255.255.255.0'
	option port '1234'

config interface 'fwdap_camera'
	option enable '1'
